#' @name uga_colors
#' @export
#'

# returns list of uga colors from palette
uga_colors <- function(n=1:8 # colors to select based on position in vector
                       ) {
  c("#BA0C2F",
    "#9EA2A2",
    "#D6D2C4",
    "#C8D8EB",
    "#554F47",
    "#E4002B",
    "#00A3AD",
    "#B4BD00",
    "#004E60")[n]
}

